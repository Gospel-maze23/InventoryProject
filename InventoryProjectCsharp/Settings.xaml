        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InventoryProjectCsharp"
        mc:Ignorable="d"
        Title="Settings" Height="300" Width="300">
    <Grid Margin="0,10,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="199*"/>
            <ColumnDefinition Width="93*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="Security" Click="Security_Click" Content="Security" HorizontalAlignment="Left" Margin="90,27,0,198" Width="109"/>
        <Button x:Name="button" Click="button_Click" Content="Email Address" HorizontalAlignment="Left" Margin="90,61,0,0" VerticalAlignment="Top" Width="109" Height="34"/>
        <Button x:Name="button1" Content="Frequency" HorizontalAlignment="Left" Margin="90,95,0,0" VerticalAlignment="Top" Width="109" Height="34"/>
        <Button x:Name="button2" Content="Threshold" HorizontalAlignment="Left" Margin="90,129,0,0" VerticalAlignment="Top" Width="109" Height="34"/>

    </Grid>
</Window>
